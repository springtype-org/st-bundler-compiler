{
    "name": "st-start",
    "version": "2.0.0-beta.1",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "st-start": "./cli.js"
    },
    "keywords": [
        "SpringType",
        "bundler",
        "compiler",
        "dev server",
        "CLI",
        "HMR",
        "node bundler",
        "sass",
        "postcss",
        "lost",
        "lost grid",
        "typescript",
        "typescript bundler",
        "babel",
        "webpack",
        "zero configuration",
        "framework agnostic"
    ],
    "description": "An opinionated, zero configuration TypeScript project bundler & starter. Supports Node.js as well.",
    "scripts": {
        "clean": "st-run st-rm-rf dist",
        "prebuild": "yarn clean",
        "build": "st-run system:tsc ++ st-cp package.json node_modules sample-config/index.html sample-config/index.tsx springtype.png dist",
        "test": "jest --verbose",
        "pree2e": "yarn test",
        "e2e": "testcafe chrome:headless **/__e2e__/*",
        "release": "npm login",
        "postrelease": "npm publish"
    },
    "author": "Aron Homberg <info@aron-homberg.de>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.21",
        "@types/node": "^12.12.3",
