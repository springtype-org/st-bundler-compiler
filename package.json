{
    "name": "st-start",
    "version": "2.0.0-alpha.15",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "st-start": "./cli.js"
    },
    "keywords": [
        "SpringType",
        "bundler",
        "start script"
    ],
    "description": "SpringType start script",
    "scripts": {
        "clean": "npx st-rm-rf dist",
        "prebuild": "yarn clean",
        "build": "tsc && cp package.json dist && cp -r node_modules dist && cp -r sample-config/index.html dist && cp sample-config/tsconfig.json dist/_tsconfig.json",
        "test": "jest --verbose",
        "pree2e": "yarn test",
        "e2e": "testcafe chrome:headless **/__e2e__/*",
        "release": "npm login",
        "postrelease": "npm publish"
    },
    "author": "Aron Homberg <info@aron-homberg.de>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.19",
        "@types/node": "^12.11.1",
