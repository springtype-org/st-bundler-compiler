{
    "name": "st-start",
    "version": "2.0.0-beta.48",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "st-start": "./cli.js"
    },
    "keywords": [
        "SpringType",
        "bundler",
        "compiler",
        "dev server",
        "CLI",
        "HMR",
        "node bundler",
        "sass",
        "postcss",
        "lost",
        "lost grid",
        "typescript",
        "typescript bundler",
        "babel",
        "webpack",
        "zero config",
        "zero configuration",
        "framework agnostic"
    ],
    "description": "An opinionated, zero configuration TypeScript project bundler & starter. Supports Node.js as well.",
    "scripts": {
        "clean": "st-run st-rm-rf dist",
        "prebuild": "yarn clean",
        "build": "st-run system:tsc ++ st-cp README.md package.json sample-config/index.html springtype.png dist ++ st-cp src/loaders/st-transform-decorator/package.json dist/loaders/st-transform-decorator ++ st-cp src/loaders/st-customize-babel-loader/package.json dist/loaders/st-customize-babel-loader",
        "test": "jest --verbose",
        "posttest": "testcafe chrome:headless **/__e2e__/*",
        "release": "npm login",
        "postrelease": "npm publish"
    },
    "bugs": {
        "url": "https://github.com/springtype-org/st-start/issues"
    },
    "author": "Aron Homberg <info@aron-homberg.de>",
    "license": "MIT",
